{
  "name": "@kemsu/react-webapp-template",
  "version": "1.0.0",
  "description": "React web-application template using rollup to build",
  "scripts": {
    "start": "node server --port 8080 --public dist/public",
    "dev": "npx run-p start \"build:client -- --dev --watch\"",
    "build:client": "node ./scripts/build-client.js",
    "build:server": "node ./scripts/build-server.js",
    "lint": "npx eslint ./src"
  },
  "rollup": {
    "inputs": [
      "index.js",
      "pages/**/index.js"
    ],
    "staticFiles": []
  },
  "serverDependencies": [
    "express",
    "minimist"
  ],
  "dependencies": {
    "express": "^4.16.4",
    "jquery": "^2.1.0",
    "minimist": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@types/jest": "^24.0.11",
    "babel-eslint": "^10.0.1",
    "chokidar": "^2.1.5",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "fast-async": "^6.3.8",
    "glob": "^7.1.3",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.3",
    "rollup-plugin-node-resolve": "^4.2.2",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "systemjs": "^3.1.1"
  },
  "babelOptions": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "exclude": [
            "transform-async-to-generator",
            "transform-regenerator"
          ]
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "module:fast-async",
        {
          "spec": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "decoratorsBeforeExport": true
        }
      ]
    ]
  },
  "keywords": [
    "app",
    "project",
    "template",
    "react",
    "rollup"
  ],
  "author": {
    "name": "Igor Sotnikov",
    "email": "sotnikov@kemsu.ru"
  },
  "license": "MIT"
}